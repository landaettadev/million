{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System": "Warning",
      "Microsoft": "Warning"
    }
  },
  "Cors": {
    "AllowedOrigins": [
      "https://staging.yourdomain.com",
      "http://localhost:3000"
    ]
  },
  "MongoDb": {
    "ConnectionString": "", // Set via environment variable MONGODB_CONNECTION_STRING
    "Database": "realestate_staging"
  },
  "Seed": {
    "Enabled": true,
    "InsertIfEmpty": true,
    "Count": 15
  },
  "Swagger": {
    "Enabled": true
  },
  "Security": {
    "EnableDetailedErrors": true,
    "EnableSensitiveDataLogging": false
  }
}
